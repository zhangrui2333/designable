{
  "name": "root",
  "private": true,
  "devEngines": {
    "node": "8.x || 9.x || 10.x || 11.x"
  },
  "workspaces": [
    "packages/*",
    "formily/*"
  ],
  "resolutions": {
    "@formily/react": "^2.2.14"
  },
  "publishConfig": {
    "access":"public"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "build": "rimraf -rf packages/*/{lib,esm} && lerna run build",
    "build:docs": "dumi build",
    "start": "dumi dev",
    "start:playground": "npm run start:basic",
    "start:basic": "yarn workspace @edan/designable-basic-example start",
    "start:sandbox": "yarn workspace @edan/designable-sandbox-example start",
    "start:multi-workspace": "yarn workspace @edan/designable-multi-workspace-example start",
    "start:sandbox-multi-workspace": "yarn workspace @edan/designable-sandbox-multi-workspace-example start",
    "start:antd": "yarn workspace @edan/designable-formily-antd start",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:prod": "jest --coverage --silent",
    "preversion": "npm run build",
    "release:force": "lerna publish from-package --yes",
    "prelease:force": "lerna publish from-package --yes --dist-tag next",
    "release": "lerna publish",
    "prelease": "lerna publish --dist-tag next",
    "lint": "eslint --ext .ts,.tsx,.js  --fix",
    "postinstall": "opencollective-postinstall"
  },
  "devDependencies": {
    "@alifd/next": "^1.26.9",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@rollup/plugin-typescript": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/dateformat": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.12",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@umijs/plugin-sass": "^1.1.1",
    "chalk": "^5.2.0",
    "concurrently": "^7.6.0",
    "conventional-commit-types": "^3.0.0",
    "cool-path": "^1.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "escape-string-regexp": "^5.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^6.1.0",
    "file-loader": "^6.2.0",
    "findup": "^0.1.5",
    "fs-extra": "^11.1.0",
    "gh-release": "^6.0.4",
    "ghooks": "^2.0.4",
    "glob": "^8.0.3",
    "immutable": "^4.1.0",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "^29.3.1",
    "jest-codemods": "^0.30.0",
    "jest-localstorage-mock": "^2.4.22",
    "jest-styled-components": "7.1.1",
    "jest-watch-lerna-packages": "^1.1.0",
    "lerna": "^6.1.0",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "moment": "^2.29.4",
    "onchange": "^7.1.0",
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "^2.0.3",
    "param-case": "^3.0.4",
    "postcss": "^8.4.19",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.1",
    "pretty-format": "^29.3.1",
    "pretty-quick": "^3.1.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.7.0",
    "rollup-plugin-external-globals": "^0.7.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "semver": "^7.3.8",
    "semver-regex": "^4.0.5",
    "showdown": "^2.1.0",
    "staged-git-files": "^1.3.0",
    "string-similarity": "^4.0.4",
    "ts-import-plugin": "2.0.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0"
  }
}

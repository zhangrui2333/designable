  },
  "dependencies": {
    "@edan/designable-shared": "workspace:0.0.1",
    "@formily/reactive": "^2.2.14",
    "@juggle/resize-observer": "^3.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
